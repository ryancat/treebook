<!DOCTYPE html>
<html>
<head>
  <title>Treebook</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="navbar">
		<div class="navbar_inner borderbox borderRadius_8">
			<div class="container">
				<%= link_to "Treebook", statuses_path, :class => "brand" %>
				<ul class="nav">
					<li><%= link_to "All Status", statuses_path %></li>
				</ul>
			</div>
		</div>
	</div>
	
	<div class="container">
		<p class="notice"><%= notice %></p>
		<p class="alert"><%= alert %></p>
		<%= yield %>
	</div>
	

</body>
</html>
